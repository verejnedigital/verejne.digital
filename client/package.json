    "qs": "^6.5.2",
    "rc-drawer": "^1.7.3",
    "react": "^16.3.2",
    "react-app-polyfill": "^0.1.3",
    "react-autocomplete": "^1.8.1",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.3",
    "react-graph-vis": "https://github.com/mirecmrozek/react-graph-vis/tarball/master",
    "react-icons": "^3.3.0",
    "react-js-pagination": "^3.0.2",
    "react-loading-components": "^1.1.12",
