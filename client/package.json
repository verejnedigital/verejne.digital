    "webfont-loader": "1.1.0",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "start-js": "BROWSER=none react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix-js": "prettier-eslint --write \"src/**/*.{js,jsx,json}\"",
    "fix-scss": "stylelint \"src/**/*.scss\" --fix",
    "fix": "npm-run-all fix-js fix-scss",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "flow": "python3 ./check_flow.py & flow & flow-coverage-report || exit 0"
  },
  "devDependencies": {
    "eslint-config-vacuumlabs": "^1.5.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.71.0",
    "flow-coverage-report": "^0.5.0",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "redux-logger": "^3.0.6",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "flow-coverage-report": {
    "includeGlob": [
      "src/**/*.js"
    ],
    "type": [
      "text",
      "html",
      "json"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
